{
  "@deathbeds/jupyterlab-deck:plugin": {
    "active": true
  },
  "@deathbeds/jupyterlab-fonts:fonts": {"styles": {
            ".MathJax_Display": {
              "body[data-jp-deck-mode='presenting'] &": {
                  "font-size": "8vh"}
              },
            "h1": {
				"body[data-jp-deck-mode='presenting'] &": {
					"color": "#fff",
					"font-size": "11vh",
					"font-weight": "bold",
					"height": "20vh",
					"letter-spacing": "normal",
					"line-height": "1.0",
					"margin": "0 0 0 0",
					"max-height": "85vh",
					"max-width": "95vw",
					"object-fit": "contain",
					"text-align": "left",
					"text-shadow": "none",
					"text-transform": "uppercase",
					"top": "20vh",
					"word-wrap": "break-word"}
			  },
			"h6": {
              "body[data-jp-deck-mode='presenting'] &": {
                  "color": "#1D8DB0",
                  "font-size": "4vh",
                  "font-weight": "bold",
                  "height": "9vh",
                  "letter-spacing": "normal",
                  "line-height": "1.0",
                  "margin": "0 0 0 0",
                  "max-height": "85vh",
                  "max-width": "95vw",
                  "object-fit": "contain",
                  "text-align": "center",
                  "text-shadow": "none",
                  "text-transform": "uppercase",
                  "top": "0vh",
                  "word-wrap": "break-word"}
              },
            "h5": {
              "body[data-jp-deck-mode='presenting'] &": {
                  "color": "#1D8DB0",
                  "font-size": "3vh",
                  "font-weight": "bold",
                  "height": "9vh",
                  "letter-spacing": "normal",
                  "line-height": "1.0",
                  "margin": "0 0 0 0",
                  "max-height": "85vh",
                  "max-width": "95vw",
                  "object-fit": "contain",
                  "text-align": "center",
                  "text-shadow": "none",
                  "text-transform": "uppercase",
                  "top": "0vh",
                  "word-wrap": "break-word"}
              },
            ".jp-RenderedSVG img": {
             "body[data-jp-deck-mode='presenting'] &": {
                  "height": "75vh",
                  "left": "0vw",
                  "object-fit": "contain",
                  "top": "0vh",
                  "width": "100vw"}
              }
           }
          }
}